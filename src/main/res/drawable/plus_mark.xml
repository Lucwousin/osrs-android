<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item>
        <shape android:shape="line">
            <stroke android:width="2dp" android:color="@color/lemon_yellow"/>
            <solid android:color="@android:color/transparent"/>
        </shape>
    </item>
    <item>
        <rotate android:fromDegrees="90" android:toDegrees="-90" android:pivotX="50%" android:pivotY="50%">
            <shape android:shape="line">
                <stroke android:width="2dp" android:color="@color/lemon_yellow"/>
                <solid android:color="@android:color/transparent"/>
            </shape>
        </rotate>
    </item>
</layer-list>
